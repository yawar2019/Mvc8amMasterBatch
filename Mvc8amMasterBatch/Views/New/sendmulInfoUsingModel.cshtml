
@{
    ViewBag.Title = "sendmulInfoUsingModel";
}

<h2>sendmulInfoUsingModel</h2>

<table class="table table-bordered table-condensed">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>

    @foreach (var item in ViewBag.Employee)
    {
        <tr>
            <td>
                @item.EmpId
            </td>
            <td>
                @item.EmpName
            </td>
            <td>
                @item.EmpSalary
            </td>
        </tr>

    }

</table>